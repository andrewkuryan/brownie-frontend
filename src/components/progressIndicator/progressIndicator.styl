@import "@colors.styl"

.progressIndicator
  width 112px
  height 112px
  position relative
  transform rotate(45deg)

.progressIndicatorLinePoint
  top 0
  left 0
  background-color primeGreen
  position absolute
  width 8px
  height 8px
  border-radius 4px
  box-shadow 0 0 5px rgba(primeGreen, 0.8)
  will-change transform
